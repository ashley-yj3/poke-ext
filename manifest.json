{
    "manifest_version": 3,
    "name": "Catch 'Em All",
    "version": "1.0.0",
    "permissions": ["tabs","activeTab","storage","alarms","scripting"],
    "background": {
        "service_worker": "background.js"
    },
    "action":{
        "default_icon": "poke_icon.png",
        "default_popup": "pokedex.html"
    },
    "content_scripts":[
        {
            "matches": ["<all_urls>"],
            "css": ["pokemon.css"],
            "js":["content.js"]
        }
    ],
    "web_accessible_resources": [
  {
    "resources": [
      "imgs/pikachu.png",
      "imgs/charizard.png",
      "imgs/garchomp.png",
      "imgs/gardevoir.png",
      "imgs/sylveon.png",
      "imgs/arceus.png",
      "imgs/rayquaza.png",
      "imgs/shroomish.png",
      "pokemon.css"
    ],
    "matches": ["<all_urls>"]
  }],
    "icons":{
        "16": "poke_icon.png",
        "48": "poke_icon.png",
        "128": "poke_icon.png"
      },
      "host_permissions": ["https://*/*"]

}