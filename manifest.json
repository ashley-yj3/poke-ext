{
    "manifest_version": 3,
    "name": "Catch 'Em All",
    "version": "1.0.0",
    "permissions": ["tabs","activeTab","storage","alarms"],
    "background": {
        "service_worker": "background.js"
    },
    "action":{
        "default_popup": "popup.html",
        "default_icon": "poke_icon.png"
    },
    "content_scripts":[
        {
            "matches": ["<all_urls>"],
            "js":["content.js"],
            "css":["stylesheet.css"]
        }
    ],
    "icons":{
        "16": "poke_icon.png",
        "48": "poke_icon.png",
        "128": "poke_icon.png"
      },
      "host_permissions": ["https://*/*"]

}